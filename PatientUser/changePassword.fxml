<AnchorPane maxHeight="550.0" maxWidth="700.0" prefHeight="800.0" prefWidth="1300.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="PatientUser.changePassword">
   <children>
      <AnchorPane prefHeight="800.0" prefWidth="1300.0" style="-fx-background-color: white;">
