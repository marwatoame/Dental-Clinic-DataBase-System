            <TextField fx:id="addPassword" layoutX="630.0" layoutY="348.0" prefHeight="45.0" prefWidth="240.0" style="-fx-background-color: powderblue;" />
            <Label alignment="CENTER" contentDisplay="CENTER" layoutX="436.0" layoutY="348.0" prefHeight="45.0" prefWidth="164.0" style="-fx-background-color: powderblue;" text="New password">
               <font>
                  <Font name="System Bold" size="22.0" />
               </font>
            </Label>
            <Label fx:id="textDisplay" alignment="CENTER" contentDisplay="CENTER" layoutX="455.0" layoutY="442.0" prefHeight="39.0" prefWidth="389.0">
               <font>
                  <Font size="18.0" />
               </font>
            </Label>
            <Button fx:id="btnChange" layoutX="429.0" layoutY="521.0" mnemonicParsing="false" onAction="#Change" prefHeight="45.0" prefWidth="442.0" style="-fx-background-color: powderblue;" text="Change">
